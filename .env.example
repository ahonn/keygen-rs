# Keygen.sh Configuration
# Copy this file to .env and fill in your actual values

# Required for all examples
KEYGEN_ACCOUNT=your-account-id
KEYGEN_API_URL=https://api.keygen.sh

# Required for admin operations (create, list, suspend, etc.)
KEYGEN_ADMIN_TOKEN=your-admin-token

# Required for client operations (validate, activate, etc.)
KEYGEN_LICENSE_KEY=your-license-key
KEYGEN_PUBLIC_KEY=your-public-key
KEYGEN_PRODUCT=your-product-id

# Required for specific examples
POLICY_ID=your-policy-id
LICENSE_ID=your-license-id
MACHINE_ID=your-machine-id